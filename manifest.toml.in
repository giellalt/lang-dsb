# Name of the speller package in various languages.
# The default is English + autonym:
[speller.name]
en = "@SPELLER_NAME_ENG@"
dsb = "@SPELLER_NAME_NATIVE@"

# Description of the speller package in various languages.
# The default is English + autonym:
[speller.description]
en = "@SPELLER_DESC_ENG@"
dsb = "@SPELLER_DESC_NATIVE@"

[macos]
system_pkg_id = "no.divvun.MacDivvun.dsb"

[windows]
system_product_code = "{851C8222-C659-4556-BDE7-2A2DD35B9B0F}_is1"

[package.speller]
name = "@GLANGUAGE@"
